{
  "name": "clipz-ai",
  "version": "1.0.0",
  "description": "AI-powered viral clips generator",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server.js",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "ytdl-core": "^4.11.5",
    "fluent-ffmpeg": "^2.1.2",
    "puppeteer": "^21.5.2",
    "openai": "^4.20.1",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "path": "^0.12.7",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": ["ai", "video", "clips", "social-media", "automation"],
  "author": "raynaythegreat",
  "license": "MIT"
}
